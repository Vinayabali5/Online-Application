package uk.ac.reigate.dto
/**
 * This interface is used to ensure that all DTO object match have a given set of methods
 * 
 * @author michael
 *
 * @param <D> The DTO Object being defined 
 * @param <O> The Object that is maps from
 */
interface DTOObject<D, O> {
    
    public D mapFromEntity(O object)
    
    public List<D> mapFromList(List<O> object)
}
